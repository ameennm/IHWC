{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "crons": [
        {
            "path": "/api/keep-alive",
            "schedule": "0 0 */3 * *"
        }
    ]
}